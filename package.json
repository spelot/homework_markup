  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "rm -rf build",
    "build": "npm run prebuild && parcel build -d ./build src/pages/index.html src/pages/settings.html src/pages/build-history.html src/pages/build-details.html --no-source-maps --no-content-hash --no-minify",
